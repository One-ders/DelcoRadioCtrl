 080808



01 00 00 00 00		= .
02 00 00 00 00		= 1
04 00 00 00 00		= STEREO
08 00 00 00 00		= :
10 00 00 00 00		= AM
20 00 00 00 00		= FM
40 00 00 00 00		= SCAN
80 00 00 00 00		= SET

00 80 00 00 00		=  50

xx 7F xx xx xx 		= second digit
xx xx 7F xx xx		= third digit
xx xx xx 7F xx		= fourth digit
